{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "methods": [
        "OPTIONS",
        "PUT",
        "POST",
        "DELETE",
        "GET"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}