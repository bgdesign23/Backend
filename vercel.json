{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": [
        "OPTIONS",
        "PUT",
        "POST",
        "DELETE",
        "GET",
        "PATCH"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, HEAD, OPTIONS",
        "Access-Control-Allow-Headers": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}